<p><%= @group.description %></p>
<table class="table">
  <thead>
    <tr>
      <th>Content</th>
      <th>Author</th>
      <th>Publish time</th>
    </tr>
  <thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.content %></td>
        <td><%= post.user.email %></td>
        <td><%= post.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
